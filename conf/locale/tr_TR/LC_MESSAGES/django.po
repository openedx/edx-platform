msgid "Transcript Languages"
msgstr "Altyazı Dilleri"

#: common/lib/xmodule/xmodule/video_module/video_xfields.py
msgid "Preferred language for transcript."
msgstr "Altyazı için tercih edilen dil."

#: common/lib/xmodule/xmodule/video_module/video_xfields.py
msgid "Preferred language for transcript"
msgstr "Altyazı için tercih edilen dil"

