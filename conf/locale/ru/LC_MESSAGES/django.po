msgstr "{id_label} № {item_id}"

#: lms/djangoapps/shoppingcart/pdf.py
#: lms/templates/emails/registration_codes_sale_invoice_attachment.txt
#, python-brace-format
msgid "Date: {date}"
msgstr "Дата: {date}"

#: lms/djangoapps/shoppingcart/pdf.py lms/djangoapps/shoppingcart/reports.py
msgid "Quantity"
msgstr "Количество"

#: lms/djangoapps/shoppingcart/pdf.py
msgid ""
"List Price\n"
"per item"
msgstr ""
"Список цен\n"
"за единицу"

#: lms/djangoapps/shoppingcart/pdf.py
